

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../favicon.ico">

   <title>{% block title %}Welcome!{% endblock %}</title>

    <!-- Bootstrap core CSS -->
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
     {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

   
  </head>

  <body>
    <div class="container">
                {% image '@AppBundle/Resources/public/images/woch.jpg' %}
                    <img src="{{ asset_url }}" alt="Logo" />
                {% endimage %}
    <div class="row" style="height:20px;"></div>
             
    </div>
    <nav class="navbar navbar-inverse">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          {% block content %}
          <a class="navbar-brand" href="/woch">{{ 'home.welcome' | trans}}</a>
          {% endblock %}
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li><a href="/woch/ziedojumi">{{ 'home.charity' | trans}}</a></li>
            <li><a href="/woch/preces">{{ 'home.products' | trans}}</a></li>
            </ul>
           <ul class="nav navbar-nav pull-right">
           <li><a href="/woch/login">{{ 'home.login' | trans}}</a></li>
           <li><a href="/woch/register">{{ 'home.register' | trans}}</a></li>
           <li><a href="/woch/registerproduct">{{ 'home.registerproduct' | trans}}</a></li>
           <li><a href="/woch/admin">{{ 'home.admin' | trans}}</a></li>
           </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>

    <div class="container">
        <div class="row">
            <div class="col-md-12">
            {% block body %}{% endblock %}
        </div>
    </div>
     
    </div><!-- /.container -->

 {% block javascripts %}{% endblock %}
   
  </body>
</html>
